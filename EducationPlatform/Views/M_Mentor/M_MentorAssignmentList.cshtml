
@{
    ViewBag.Title = "M_MentorAssignmentList";
}

<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title></title>
</head>
<body>
    <center>
        <h2 class="fw-bolder text-uppercase">AssignmentList</h2>


        <table class="table table-hover">
            <thead>
                <tr class="table-dark  text-center text-uppercase fw-bolder">
                    <th scope="col">Assignment ID</th>
                    <th scope="col">Question</th>
                    <th scope="col">Date</th>
                    <th></th>
                </tr>
            </thead>
            <tbody class="table-group-divider">

                @foreach (var p in Model)
                {
                    <tr class="text-center  fw-bolder">
                        <th scope="row">@p.Id</th>

                        <th>@p.Question</th>
                        <th>@p.Date</th>
                        <th>
                            <a class="btn btn-warning" href="~/M_Mentor/M_MentorTrackStudent/@p.Id" role="button">Submitted StudentList</a>
                        </th>
                    </tr>


                }


            </tbody>
        </table>
    </center>
    </body>
</html>