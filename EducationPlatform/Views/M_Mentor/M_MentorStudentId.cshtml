
@{
    ViewBag.Title = "M_MentorStudentId";
}

<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title></title>
</head>
<body>
    <center>
        <h2 class="fw-bolder text-uppercase">Student</h2>


        <table class="table table-hover">
            <thead>
                <tr class="table-dark  text-center text-uppercase fw-bolder">
                    <th scope="col">STUDENT ID</th>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody class="table-group-divider">

                @foreach (var p in Model)
                {
                    <tr class="text-center  fw-bolder">
                        <th scope="row">@p.StudentId</th>



                        <td>
                            <a class="btn btn-info text-uppercase  fw-bolder" href="~/M_Mentor/M_MentorStudentList/@p.StudentId" role="button">STUDENT INFORMATION</a>

                        </td>

                        <td>
                            <a class="btn btn-info text-uppercase  fw-bolder" href="~/M_Mentor/M_MentorStudentReview/@p.StudentId" role="button">REVIEW STUDENTS</a>
                        </td>




                        <td>
                            <a class="btn btn-info text-uppercase  fw-bolder" href="~/M_Mentor/M_MentorStudentCertificate/@p.StudentId" role="button">RECOMMAND CERTIFICATE</a>
                        </td>
                    </tr>

                }


            </tbody>
        </table>
    </center>
</body>
</html>