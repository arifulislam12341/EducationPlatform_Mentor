
@{
    ViewBag.Title = "M_MentorSeeUploadModule";
}



<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title></title>
</head>
<body>
    <center><h1 class="fw-bolder">SeeUploadModule</h1></center>
    @{
        if (Model.Count == 0)
        {

            <h1 class="text-center text-uppercase fw-bolder">No module uploaded</h1>

        }
        else
        {
<table class="table table-hover">
    <thead>
        <tr class="table-dark fs-4 text-center text-uppercase fw-bolder">
            <th scope="col">Material</th>
            <th scope="col">module</th>
            <th scope="col">description</th>
            <th scope="col">date</th>




        </tr>
    </thead>
    <tbody class="table-group-divider">

        @foreach (var p in Model)
        {
            <tr class="text-center fs-5 fw-bolder">

                <td>@p.Material</td>
                <td>@p.Module</td>
                <td>@p.Description</td>
                <td>@p.Date</td>



            </tr>
        }

    </tbody>
</table>
                }


                }


       

        </body>
</html>