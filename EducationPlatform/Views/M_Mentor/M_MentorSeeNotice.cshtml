
@{
    ViewBag.Title = "M_MentorSeeNotice";
}

<h2>M_MentorSeeNotice</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">NOTICE</th>
            
            <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody class="table-group-divider">

        @foreach (var p in Model)
        {
            <tr>
               

                <th>@p.Details</th>
                <th>@p.Date</th>
              
            </tr>


        }


    </tbody>
</table>