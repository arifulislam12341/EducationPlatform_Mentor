
@{
    ViewBag.Title = "M_MentorCourseDetails";
}
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title></title>
</head>
<body>

    <h2 class="text-center text-uppercase fw-bolder">Course Details</h2>


    <table class="table table-hover">
        <thead>
            <tr class="table-dark  text-center text-uppercase fw-bolder">
                <th scope="col">COURSE ID</th>
                <th scope="col"> COURSE NAME</th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody class="table-group-divider">





            @foreach (var i in Model)
            {

                <tr class="text-center fw-bolder">
                    <td>@i.Id</td>
                    <td>@i.Name</td>



                    <td> <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorAssignment/@i.Id" role="button">ASSAIGN ASSIGNMENT</a></td>
                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorStudentId/@i.Id" role="button">STUDENTS</a>
                    </td>
                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorCounsiling/@i.Id" role="button">GIVE COUNSILING</a>
                    </td>

                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorNotice/@i.Id" role="button">POST NOTICE</a>
                    </td>
                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorAssignmentList/@i.Id" role="button">POST RESULT</a>
                    </td>
                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorRatings/@i.Id" role="button">VIEW RATINGS</a>
                    </td>
                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorAllStudentResults/@i.Id" role="button">VIEW RESULTS</a>
                    </td>

                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorUploadModule/@i.Id" role="button">UPLOAD MODULE</a>

                    </td>

                    <td>
                        <a class="btn btn-secondary  text-uppercase fw-bolder" href="~/M_Mentor/M_MentorSeeUploadModule/@i.Id" role="button">SEE MODULE</a>

                    </td>
                </tr>
            }
        </tbody>
    </table>

    </body>
</html>