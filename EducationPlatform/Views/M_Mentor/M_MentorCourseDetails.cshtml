
@{
    ViewBag.Title = "M_MentorCourseDetails";
}

<h2>M_MentorCourseDetails</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>


        </tr>
    </thead>
    <tbody class="table-group-divider">





        @foreach (var i in Model)
        {

            <tr>
                <td>@i.Id</td>
                <td>@i.Name</td>



                <td> <a class="btn btn-warning" href="~/M_Mentor/M_MentorAssignment/@i.Id" role="button">ASSAIGN ASSIGNMENT</a></td>
                <td>
                    <a class="btn btn-danger" href="~/M_Mentor/M_MentorStudentId/@i.Id" role="button">STUDENTS</a>
                </td>
                <td>
                    <a class="btn btn-danger" href="~/M_Mentor/M_MentorCounsiling/@i.Id" role="button">GiveCounsiling</a>
                </td>

                <td>
                    <a class="btn btn-danger" href="~/M_Mentor/M_MentorNotice/@i.Id" role="button">POST NOTICE</a>
                </td>
                <td>
                    <a class="btn btn-danger" href="~/M_Mentor/M_MentorAssignmentList/@i.Id" role="button">POST RESULT</a>
                </td>
                <td>
                    <a class="btn btn-danger" href="~/M_Mentor/M_MentorRatings/@i.Id" role="button">VIEW RATINGS</a>
                </td>
                <td>
                    <a class="btn btn-danger" href="~/M_Mentor/M_MentorAllStudentResults/@i.Id" role="button">VIEW RESULTS</a>
                </td>
            </tr>
        }
        </tbody>
    </table>

